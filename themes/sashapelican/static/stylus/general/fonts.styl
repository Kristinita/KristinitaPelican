@font-face
	font-family 'AleksandraC'
	src local('AleksandraCRegular'), url('../../fonts/AleksandraC/aleksandracregular.woff2') format('woff2'), url('../../fonts/AleksandraC/aleksandracregular.woff') format('woff'), url('../../fonts/AleksandraC/aleksandracregular.ttf') format('truetype')
	font-weight 400

@font-face
	font-family 'Истина'
	src url('../../fonts/Истина/Истина.eot')
	src url('../../fonts/Истина/Истина.eot?#iefix') format('embedded-opentype'),
    url('../../fonts/Истина/Истина.woff') format('woff'),
    url('../../fonts/Истина/Истина.ttf') format('truetype')
	/* Decrease text brightness */
	-webkit-filter brightness(97%)
	filter brightness(97%)

@font-face
	font-family 'Мечта'
	font-weight 500
	font-style italic
	src url('../../fonts/Мечта/Мечта.eot')
	src url('../../fonts/Мечта/Мечта.eot?#iefix') format('embedded-opentype'),
    url('../../fonts/Мечта/Мечта.woff') format('woff'),
    url('../../fonts/Мечта/Мечта.ttf') format('truetype')
	-webkit-filter brightness(97%)
	filter brightness(97%)

@font-face
	font-family 'Кира_Бог'
	src url('../../fonts/Кира_Бог/Кира_Бог.eot')
